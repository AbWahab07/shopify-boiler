}
.main-nav nav .top-level a {
  color: #b300b3;
  font-size: 16px;
  font-weight: 700;
  text-transform: uppercase;
  display: inline-block;
  transition: all 150ms ease-out;
}
.main-nav nav .top-level.active, .main-nav nav .top-level:hover {
  background-color: #cd00cd;
}
.main-nav nav .top-level.active a, .main-nav nav .top-level:hover a {
  color: #e6e6e6;
}
.main-nav nav .sub-nav {
  display: none;
