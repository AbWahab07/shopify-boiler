  line-height: 2rem;
}
@media(max-width:768px) {
  .mobile-nav {
    display: block;
  }
}
.main-nav {
  position: relative;
  top: 45px;
}
.main-nav nav {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  text-align: center;
  background-color: #e6e6e6;
  transition: all 150ms ease-out;
  font-size: 0;
}
.main-nav nav .top-level {
  display: inline-block;
