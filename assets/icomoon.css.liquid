@font-face {
    font-family: 'icomoon';
    src:    url({{ 'icomoon.eot?35eal9' | asset_url }});
    src:    url({{ 'icomoon.eot?35eal9#iefix' | asset_url }}) format('embedded-opentype'),
        url({{ 'icomoon.ttf?35eal9' | asset_url }}) format('truetype'),
        url({{ 'icomoon.woff?35eal9' | asset_url }}) format('woff'),
        url({{ 'icomoon.svg?35eal9#icomoon' | asset_url }}) format('svg');
    font-weight: normal;
    font-style: normal;
}

[class^="spr-icon-"]:before, [class*=" spr-icon-"]:before,
[class^="icon-"], [class*=" icon-"] {
    /* use !important to prevent issues with browser extensions that change fonts */
    font-family: 'icomoon' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    line-height: 1;

    /* Better Font Rendering =========== */
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}


